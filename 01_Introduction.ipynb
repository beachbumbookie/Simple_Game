    "\n",
    "fig = px.line(\n",
    "    df_long,\n",
    "    x='Year', y='Population', color=\"Area\",\n",
    "    title=\"Predicted Population and Average Daily Vistors for Unincorporated Clark County 1950 to 1980\",\n",
    "    labels={'Population' : \"Population\"}\n",
    ")\n",
    "\n",
    "fig.update_traces(\n",
    "    hovertemplate=(\n",
    "        \"Area = %{fullData.name}<br>\"\n",
    "        \"Year = %{x}<br>\"\n",
